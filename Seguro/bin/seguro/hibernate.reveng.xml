<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
	<type-mapping>
		<sql-type jdbc-type="INTEGER" hibernate-type="int"
			not-null="true">
		</sql-type>
		<sql-type jdbc-type="TIMESTAMP" hibernate-type="timestamp"
			length="19" not-null="false">
		</sql-type>
		<sql-type jdbc-type="INTEGER" hibernate-type="java.lang.Integer"
			not-null="false">
		</sql-type>
		<sql-type jdbc-type="INTEGER" hibernate-type="java.lang.Integer"
			not-null="false">
		</sql-type>
		<sql-type jdbc-type="VARCHAR" hibernate-type="string"
			not-null="false">
		</sql-type>
		<sql-type jdbc-type="VARCHAR" hibernate-type="string"
			not-null="true">
		</sql-type>
		<sql-type jdbc-type="CHAR" hibernate-type="java.lang.Character"
			length="1" not-null="false">
		</sql-type>
		<sql-type jdbc-type="INTEGER" hibernate-type="java.lang.Integer"
			not-null="false">
		</sql-type>
		<sql-type jdbc-type="VARCHAR" hibernate-type="string"
			not-null="false">
		</sql-type>
		<sql-type jdbc-type="VARCHAR" hibernate-type="string"
			not-null="false">
		</sql-type>
		<sql-type jdbc-type="VARCHAR" hibernate-type="string"
			not-null="false">
		</sql-type>
	</type-mapping>


	<table-filter match-catalog="seguros" match-name="seguro" />
	<table catalog="seguros" name="seguro">
		<column name="idSeguro"></column>
		<column name="ape1"></column>
		<column name="nombre"></column>
		<column name="ape2"></column>
		<column name="edad"></column>
		<column name="fechaCreacion"></column>
		<column name="casado"></column>
		<column name="numHijos"></column>
		<column name="tipoSeguro"></column>
		<column name="sexo"></column>
		<column name="nif"></column>
	</table>
</hibernate-reverse-engineering>